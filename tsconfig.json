{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "ES2016",
         "ES2018",
      ],
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
   },
   "include": [
      "src/**/*"
   ]
}